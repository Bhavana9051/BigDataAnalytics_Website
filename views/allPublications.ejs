<head>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body class="bg-info">
  <div class="container">
      <div class="row">
          <div class="col-md-8 offset-md-2" style="background-color: #fff; margin-top: 25px; padding: 20px;">
              <h3><a class="btn btn-secondary"><i class="fa fa-file-text-o fa-lg"></i></a>All Publications List</h3>
              <div class="table-responsive">
                  <table class="table table-striped">
                      <thead>
                          <tr>
                              <th>User Name</th>
                              <th>Email</th>
                              <th>Topic</th>
                              <th>Keywords</th>
                              <th>URL</th>
                              <th></th>
                          </tr>
                      </thead>
                      <tbody>
                          <% list.forEach(function(publication) { %>
                          <tr>
                              <td><%= publication.username %></td>
                              <td><%= publication.email %></td>
                              <td><%= publication.topic %></td>
                              <td><%= publication.keywords %></td>
                              <td><a href="<%= publication.url %>"><%= publication.url %></a></td>
                          </tr>
                          <% }); %>
                      </tbody>
                  </table>
              </div>
          </div>
      </div>
  </div>
</body>

 
 
 
 
 